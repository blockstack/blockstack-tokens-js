{
  "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "declaration": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "outDir": "lib",
      "sourceMap": true,
      "sourceRoot": "."
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/test/**/*"
  ]
}
