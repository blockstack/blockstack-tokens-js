{
  "name": "jsontokens",
  "version": "3.0.0-alpha.0",
  "description": "node.js library for encoding, decoding, and verifying JSON Web Tokens (JWTs)",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint --ext .ts ./src",
    "test": "npm run lint && jest",
    "codecovUpload": "codecov",
    "prepublishOnly": "npm run compile",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockstack/jsontokens-js.git"
  },
  "keywords": [
    "jwt",
    "json",
    "web",
    "token",
    "encode",
    "decode",
    "verify",
    "ecdsa",
    "secp256k1",
    "ec",
    "elliptic",
    "curve",
    "signature",
    "sign"
  ],
  "author": "Blockstack PBC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blockstack/jsontokens-js/issues"
  },
  "homepage": "https://github.com/blockstack/jsontokens-js#readme",
  "devDependencies": {
    "@peculiar/webcrypto": "^1.0.21",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.7.12",
    "@types/tape": "^4.2.33",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "codecov": "^3.5.0",
    "eslint": "^6.5.1",
    "eslint-plugin-jest": "^22.19.0",
    "istanbul": "^0.4.5",
    "jest": "^24.9.0",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.13",
    "tape": "^4.11.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@types/elliptic": "^6.4.9",
    "asn1.js": "^5.0.1",
    "base64url": "^3.0.1",
    "ecdsa-sig-formatter": "^1.0.11",
    "elliptic": "^6.4.1",
    "key-encoder": "^2.0.3-beta.0"
  },
  "files": [
    "lib/",
    "src/",
    "README.md"
  ],
  "nyc": {
    "cache": false,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/test/**"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
