{
  "name": "jsontokens",
  "version": "2.0.0",
  "description": "node.js library for encoding, decoding, and verifying JSON Web Tokens (JWTs)",
  "main": "lib/index.js",
  "scripts": {
    "compile": "tsc",
    "lint": "eslint --ext .ts ./src",
    "test": "npm run lint && nyc node ./src/test/index.ts",
    "codecovUpload": "codecov",
    "prepublishOnly": "npm run compile",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockstack/jsontokens-js.git"
  },
  "keywords": [
    "jwt",
    "json",
    "web",
    "token",
    "encode",
    "decode",
    "verify",
    "ecdsa",
    "secp256k1",
    "ec",
    "elliptic",
    "curve",
    "signature",
    "sign"
  ],
  "author": "Blockstack PBC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blockstack/jsontokens-js/issues"
  },
  "homepage": "https://github.com/blockstack/jsontokens-js#readme",
  "devDependencies": {
    "@types/elliptic": "^6.4.6",
    "@types/node": "^10.14.6",
    "@types/tape": "^4.2.33",
    "@types/validator": "^10.11.0",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "codecov": "^3.3.0",
    "eslint": "^5.16.0",
    "istanbul": "^0.4.5",
    "nyc": "^13.3.0",
    "source-map-support": "^0.5.12",
    "tape": "^4.10.1",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "asn1.js": "^5.0.1",
    "base64url": "^3.0.1",
    "ecdsa-sig-formatter": "^1.0.11",
    "elliptic": "^6.4.1",
    "key-encoder": "github:blockstack/key-encoder-js#feature/typescript-port",
    "validator": "^10.11.0"
  },
  "files": [
    "lib/",
    "src/",
    "README.md"
  ],
  "nyc": {
    "cache": false,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/test/**"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ],
    "reporter": [
      "text",
      "html",
      "lcov"
    ]
  }
}
